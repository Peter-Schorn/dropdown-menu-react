{
    "extends": "./tsconfig.options.json",
    "compilerOptions": {
        "types": [
            "node"
        ],
        "composite": true,
        "tsBuildInfoFile": "./node_modules/.cache/tsconfig.node.tsbuildinfo",
        "noEmit": true,
        "lib": [
            "es2020"
        ],
        "allowJs": true,
        "checkJs": true
    },
    "include": [
        "vite.config.ts",
        "eslint.config.ts",
        "scripts/**/*",
        "svgr-template.ts"
    ]
}
